{
  "name": "edurka",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dbsl": "dotenv -e .env.development.local -- npx prisma studio",
    "dbsp": "dotenv -e .env.production.local -- npx prisma studio",
    "dbg": "npx zenstack generate",
    "db": "pg_ctl start",
    "dbs": "pg_ctl stop",
    "dbr": "pg_ctl restart",
    "dbml": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "dbmr": "dotenv -e .env.development.local -- npx prisma migrate reset",
    "test": "bun '.\test.ts'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 180,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "dependencies": {
    "@libsql/client": "^0.6.2",
    "@prisma/adapter-pg": "^5.15.0",
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.40.1",
    "@tiptap/extension-text-align": "^2.4.0",
    "@tiptap/react": "^2.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/nodemailer": "^6.4.15",
    "@vercel/postgres": "^0.8.0",
    "@zenstackhq/runtime": "^2.2.2",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.2.10",
    "geist": "^1.3.0",
    "html-react-parser": "^5.1.10",
    "jose": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.3",
    "nodemailer": "^6.9.13",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "sass": "^1.77.4",
    "valtio": "^1.13.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.0-beta",
    "zenstack": "^2.2.2"
  }
